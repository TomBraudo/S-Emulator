<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="200.0" prefWidth="250.0" stylesheets="../../../app.css" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.app.ui.dashboard.components.function.FunctionLineController">
   <children>
      <VBox fx:id="root" layoutX="4.0" prefHeight="200.0" prefWidth="399.0" styleClass="card">
         <children>
            <HBox prefHeight="17.0" prefWidth="376.0">
               <children>
                  <Label prefHeight="17.0" prefWidth="133.0" styleClass="label-strong" text="Function Name:" />
                  <Label fx:id="nameLabel" prefHeight="17.0" prefWidth="236.0" styleClass="command-label" text="name" />
               </children>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label prefHeight="17.0" prefWidth="133.0" styleClass="label-strong" text="Source Program:" />
                  <Label fx:id="sourceProgramLabel" prefHeight="17.0" prefWidth="236.0" styleClass="command-label" text="source" />
               </children>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label prefHeight="17.0" prefWidth="133.0" styleClass="label-strong" text="Uploader Username:" />
                  <Label fx:id="usernameLabel" prefHeight="17.0" prefWidth="236.0" styleClass="command-label" text="username" />
               </children>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label prefHeight="17.0" prefWidth="133.0" styleClass="label-strong" text="Commands Count:" />
                  <Label fx:id="commandsCountLabel" prefHeight="17.0" prefWidth="236.0" styleClass="command-label" text="count" />
               </children>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label prefHeight="17.0" prefWidth="133.0" styleClass="label-strong" text="Max Expansion Level:" />
                  <Label fx:id="maxExpansionLevelLabel" prefHeight="17.0" prefWidth="236.0" styleClass="command-label" text="level" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
