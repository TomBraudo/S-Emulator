<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="200.0" prefWidth="250.0" stylesheets="../../../app.css" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.app.ui.dashboard.components.user.UserLineController">
   <children>
      <VBox fx:id="root" layoutX="4.0" prefHeight="200.0" prefWidth="235.0" styleClass="card">
         <children>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label prefHeight="17.0" prefWidth="150.0" styleClass="label-strong" text="User:" />
                  <Label fx:id="userNameLabel" prefHeight="17.0" prefWidth="64.0" styleClass="command-label" text="name" />
               </children>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label prefHeight="17.0" prefWidth="150.0" styleClass="label-strong" text="Programs Uploaded: " />
                  <Label fx:id="programsUploadedLabel" prefHeight="17.0" prefWidth="64.0" styleClass="command-label" text="Count" />
               </children>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label prefHeight="17.0" prefWidth="150.0" styleClass="label-strong" text="Functions Uploaded:" />
                  <Label fx:id="functionsUploadedLabel" prefHeight="17.0" prefWidth="64.0" styleClass="command-label" text="Count" />
               </children>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label prefHeight="17.0" prefWidth="150.0" styleClass="label-strong" text="Credits:" />
                  <Label fx:id="creditsLabel" prefHeight="17.0" prefWidth="64.0" styleClass="command-label" text="Count" />
               </children>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label prefHeight="17.0" prefWidth="150.0" styleClass="label-strong" text="Credits used:" />
                  <Label fx:id="creditsUsedLabel" prefHeight="17.0" prefWidth="64.0" styleClass="command-label" text="Count" />
               </children>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label prefHeight="17.0" prefWidth="150.0" styleClass="label-strong" text="Runs: " />
                  <Label fx:id="runsLabel" prefHeight="17.0" prefWidth="64.0" styleClass="command-label" text="Count" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
